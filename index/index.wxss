/* tabBar/home/home.wxss */
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    -webkit-text-size-adjust:none;
    -webkit-user-select: none;
    font-size: 28rpx;
    color: #333;
    font-family: Helvetica,Arial,"Helvetica Neue","Droid Sans","Microsoft YaHei",sans-serif;
    padding: 30rpx;
}
.canvas {
    width: 100%;
    height: 200px;
}
.homebox .title {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  justify-items: center;
  -webkit-justify-content: center;
  -webkit-justify-items: center;
	text-align: left;
	position: relative;
	margin-top: 20rpx;
	margin-bottom: 20rpx;
}
.homebox .title>view{
  width: 50%;
}
.homebox .title>view.side_r{
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  justify-items: center;
  -webkit-justify-content: center;
  -webkit-justify-items: center;
}
.homebox .title>view.side_r>view{
  width: 60rpx;
  text-align: center;
}
.homebox .title>view.side_r>view.on{
  color:#47CCE2;
}
.back-btn {
	position: absolute;
	left: 20rpx;
	top: 0;
}